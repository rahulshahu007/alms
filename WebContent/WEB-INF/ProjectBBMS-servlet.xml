<?xml version="1.0" encoding="UTF-8"?>

<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean name="/FarmerController.spring" class="Contoller.FarmerController"/>
<bean name="/LaborController.spring" class="Contoller.LaborController"/>
<bean name="/FarmerRegController.spring" class="Contoller.FarmerRegController"/>
<bean name="/RequestController.spring" class="Contoller.RequestController"/>

	<bean id="viewResolver" class="org.springframework.web.servlet.view.InternalResourceViewResolver">
	
		<property name="prefix">
		<value>/</value>
		</property>
		<property name="suffix">
		<value>.jsp</value>
		</property>
		
	</bean>
	
</beans>